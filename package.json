{
  "name": "material-ui-example-webpack",
  "version": "0.15.0",
  "description": "Sample project that uses Material-UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "serve": "node server.js",
    "lint": "./node_modules/.bin/eslint src",
    "test2": "mocha --compilers js:babel-register --recursive",
    "test": "mocha-webpack --webpack-config webpack.config-test.js \"test/**/*.spec.js\"",
    "test:watch": "npm test -- --watch"
  },
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.6.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.2.0",
    "rimraf": "^2.5.3",
    "sw-precache-webpack-plugin": "^0.3.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-node-externals": "^1.3.3",
    "webpack-path-rewriter": "^1.1.4"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "immutable": "^3.8.1",
    "install": "^0.8.1",
    "json-loader": "^0.5.4",
    "local-t2-app-redux": "0.0.30",
    "local-t2-device-redux": "0.0.5",
    "local-t2-navigation-redux": "0.0.3",
