    "react-dom": "^15.0.1",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-swipeable-views": "^0.6.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.6",
    "redux-persist": "^3.2.2",
    "redux-persist-migrate": "^4.0.1",
    "redux-responsive": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
